@model ProdutoModel
<h1>Produto - Detalhes</h1>

<div class="container">
	<div class="row">
		<div class="col-lg-12">
			<div class="well">
				<input type="hidden" name="ProdutoId" value="@Model.ProdutoId" />
				<div class="form-group">
					<label asp-for="NomeProduto" class="form-label"></label>
					<p>@Model.NomeProduto</p>
				</div>
				<div class="form-group">
					<label asp-for="Descricao" class="form-label"></label>
					<p>@Model.Descricao</p>
				</div>
			</div>
		</div>
	</div>
</div>